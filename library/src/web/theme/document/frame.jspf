<imthemedef:section name='frameStart'>
  <div class='documentThemeFrame PROPERTY(class)' ATTRIBUTE(id) ATTRIBUTE(style)>
</imthemedef:section>
<imthemedef:section name='framePaneStart'>
    <div class='documentThemeFramePane PROPERTY(class)' ATTRIBUTE(id) ATTRIBUTE(style)>
</imthemedef:section>
<imthemedef:section name='frameBreak'>
    </div>
    <div class='documentThemeFramePane PROPERTY(class)' ATTRIBUTE(id) ATTRIBUTE(style)>
</imthemedef:section>
<imthemedef:section name='framePaneEnd'>
    </div>
</imthemedef:section>
<imthemedef:section name='frameEnd' >
  </div>
</imthemedef:section>
<imthemedef:section name='buttonFrameStart'>
  <div ATTRIBUTE(id,buttonFrame) class='documentThemeButtonFrame PROPERTY(class)' ATTRIBUTE(style)>
</imthemedef:section>
<imthemedef:section name='buttonSpacer'>
  <div ATTRIBUTE(id) class='documentThemeButtonSpacer PROPERTY(class)' ATTRIBUTE(style)>&nbsp;</div>
</imthemedef:section>
<imthemedef:section name='buttonFrameEnd'>
  </div>
</imthemedef:section>
